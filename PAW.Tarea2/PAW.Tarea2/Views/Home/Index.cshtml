@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Tarea 2 WCF</h1>
    <p class="lead">WCF, ASP.net</p>
    
</div>

<div class="container">
    <div class="img-responsive">
        <img src=".Images/ECUACION.gif" />
    </div>
    <div class="form-horizontal">
        <form action="/" method="post">
            <div class="row col-md-6 form-inline ">
                <div class=" ">
                    <label>A: </label>
                    <input class="form-control" type="number" name="a" value="" />
                </div>

                <div class="">
                    <label>B: </label>
                    <input class="form-control" type="number" name="b" value="" />
                </div>

                <div class="">
                    <label>C: </label>
                    <input class="form-control" type="number" name="c" value="" />
                </div>
                <br />
                <div class="">
                    <label>X1: </label>
                    <input class="form-control" type="number" name="x1" readonly value="" />
                </div>
                <div class="">
                    <label>X2: </label>
                    <input class="form-control" type="number" name="x2" readonly value="" />
                </div>
                <div class="btn">
                    <input class=" btn btn-success" type="button" name="btn_Calcular" value="Calcular"  />
                </div>
            </div>
        </form>
    </div>
</div>